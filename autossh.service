[Unit]
Description=Keeps a tunnel to the remote access server open
After=network.target

[Service]
User=autossh
ExecStart=/usr/bin/autossh -M 0 -N -q -o "ServerAliveInterval 60" -o "ServerAliveCountMax 3" -p 22 -l raspberry 52.18.36.77 -R 9001:localhost:22 -i /home/autossh/.ssh/id_rsa

[Install]
WantedBy=multi-user.target

